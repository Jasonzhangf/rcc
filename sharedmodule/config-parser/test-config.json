{
  "version": "1.0.0",
  "providers": {
    "test-provider": {
      "name": "Test Provider",
      "type": "api",
      "endpoint": "https://api.example.com",
      "models": {
        "test-model": {
          "name": "Test Model",
          "contextLength": 4096,
          "supportsFunctions": true
        }
      },
      "auth": {
        "type": "api-key",
