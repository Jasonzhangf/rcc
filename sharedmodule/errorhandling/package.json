  },
  "files": [
    "dist/**/*",
    "index.ts",
    "types/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rcc/rcc-errorhandling.git"
  },
  "bugs": {
