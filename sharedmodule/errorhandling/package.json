{
  "name": "rcc-errorhandling",
  "version": "1.0.5",
  "description": "RCC ErrorHandling Center - Simple error handling for RCC modules",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": ["dist/index.d.ts"]
    }
  },
  "scripts": {
    "build": "tsc --outDir dist",
    "dev": "tsc --watch",
    "test": "echo \"No tests yet\"",
