{
  "name": "openai-compatible-providers-framework",
  "version": "0.0.10",
  "description": "OpenAI Compatible Providers Framework with Qwen and iFlow tool calling support, OAuth authentication, dynamic module loading, and comprehensive debug logging system",
  "main": "dist/index-standalone.js",
  "types": "dist/index-standalone.d.ts",
  "scripts": {
    "build": "npx tsc -p tsconfig-standalone.json",
    "test": "jest",
    "test:debug": "ts-node test/debug-logging-test.ts",
    "test:integration": "npm run test:debug",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepublishOnly": "npx tsc -p tsconfig-standalone.json",
    "publish:patch": "npm version patch && npm publish",
    "publish:minor": "npm version minor && npm publish",
    "publish:major": "npm version major && npm publish",
    "clean": "rm -rf dist",
    "clean:logs": "rm -rf test-logs logs",
    "dev": "ts-node src/index.ts",
    "typecheck": "tsc --noEmit",
    "build:watch": "tsc --watch"
  },
  "keywords": [
    "openai",
    "providers",
    "framework",
    "compatibility",
    "ai",
    "chat",
    "llm",
    "basemodule",
    "rcc",
    "qwen",
    "iflow",
    "tool-calling",
    "oauth",
    "authentication",
    "debug-logging",
    "logging",
    "monitoring",
    "performance",
    "request-tracking",
    "pipeline-logging",
    "error-tracking"
  ],
  "author": "WebAuto Team",
  "license": "MIT",
  "dependencies": {
    "@types/axios": "^0.9.36",
    "axios": "^1.12.2",
    "open": "^10.2.0",
    "rcc-basemodule": "^0.1.4",
    "rcc-errorhandling": "^1.0.5"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.19.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Jasonzhangf/webauto.git",
    "directory": "sharedmodule/openai-compatible-providers"
  },
  "bugs": {
    "url": "https://github.com/Jasonzhangf/webauto/issues"
  },
  "homepage": "https://github.com/Jasonzhangf/webauto#readme"
}
