{
  "metadata": {
    "description": "Provider-specific transformation configurations extracted from claude-code-router",
    "source": "claude-code-router provider configurations",
    "created": "2025-01-10",
    "version": "1.0.0"
  },
  "providers": {
    "siliconflow": {
      "apiBase": "https://api.moonshot.cn/v1/chat/completions",
      "models": ["kimi-k2-0711-preview"],
      "transformations": {
        "maxTokenAdjustment": {
          "type": "mapping",
          "max_tokens": 130000
        }
      }
    },
    "kimi": {
      "apiBase": "https://api.moonshot.cn/v1/chat/completions",
      "models": ["kimi-k2-0711-preview"],
      "transformations": {
        "directMapping": {
          "type": "passthrough"
        }
      }
    },
    "groq": {
      "apiBase": "https://api.groq.com/openai/v1/chat/completions",
      "models": ["moonshotai/kimi-k2-instruct"],
      "transformations": {
        "maxTokenAdjustment": {
          "type": "mapping",
          "max_tokens": 16384
        },
        "groqSpecific": {
          "type": "provider_specific",
          "provider": "groq"
        }
      }
    },
    "openrouter": {
      "apiBase": "https://openrouter.ai/api/v1/chat/completions",
      "models": [
        "google/gemini-2.5-pro-preview",
        "anthropic/claude-sonnet-4",
        "anthropic/claude-3.5-sonnet",
        "anthropic/claude-3.7-sonnet:thinking",
        "deepseek/deepseek-chat-v3-0324",
        "@preset/kimi"
      ],
      "transformations": {
        "openrouterSpecific": {
          "type": "provider_specific",
          "provider": "openrouter"
        },
        "deepseekToolUse": {
          "type": "tool_use_mapping",
          "model": "deepseek/deepseek-chat-v3-0324",
          "transformations": ["tooluse"]
        }
      }
    },
    "deepseek": {
      "apiBase": "https://api.deepseek.com/chat/completions",
      "models": ["deepseek-chat", "deepseek-reasoner"],
      "transformations": {
        "deepseekSpecific": {
          "type": "provider_specific",
          "provider": "deepseek"
        },
        "deepseekToolUse": {
          "type": "tool_use_mapping",
          "model": "deepseek-chat",
          "transformations": ["tooluse"]
        }
      }
    },
    "test": {
      "apiBase": "https://tbai.xin/v1/chat/completions",
      "models": ["gemini-2.5-pro"],
      "transformations": {
        "directMapping": {
          "type": "passthrough"
        }
      }
    },
    "ollama": {
      "apiBase": "http://localhost:11434/v1/chat/completions",
      "models": ["qwen2.5-coder:latest"],
      "transformations": {
        "ollamaSpecific": {
          "type": "provider_specific",
          "provider": "ollama"
        }
      }
    },
    "gemini": {
      "apiBase": "https://generativelanguage.googleapis.com/v1beta/models/",
      "models": ["gemini-2.5-flash", "gemini-2.5-pro"],
      "transformations": {
        "geminiSpecific": {
          "type": "provider_specific",
          "provider": "gemini"
        }
      }
    },
    "volcengine": {
      "apiBase": "https://ark.cn-beijing.volces.com/api/v3/chat/completions",
      "models": ["deepseek-v3-250324", "deepseek-r1-250528"],
      "transformations": {
        "deepseekMapping": {
          "type": "provider_specific",
          "provider": "deepseek"
        }
      }
    },
    "gemini-cli": {
      "apiBase": "https://cloudcode-pa.googleapis.com/v1internal",
      "models": ["gemini-2.5-flash", "gemini-2.5-pro"],
      "transformations": {
        "geminiCliSpecific": {
          "type": "provider_specific",
          "provider": "gemini-cli"
        }
      }
    },
    "azure": {
      "apiBase": "https://your-resource-name.openai.azure.com/",
      "models": ["gpt-4"],
      "transformations": {
        "azureSpecific": {
          "type": "provider_specific",
          "provider": "azure"
        }
      }
    }
  },
  "transformationPatterns": {
    "maxTokenAdjustment": {
      "description": "Adjust max_tokens parameter",
      "type": "parameter_mapping",
      "field": "max_tokens",
      "transform": "set_value"
    },
    "providerSpecific": {
      "description": "Provider-specific transformations",
      "type": "custom",
      "handler": "provider_specific_handler"
    },
    "toolUseMapping": {
      "description": "Tool usage mapping for specific models",
      "type": "conditional",
      "condition": "model_match",
      "transformations": ["tool_use_format"]
    },
    "passthrough": {
      "description": "Direct mapping without transformation",
      "type": "identity"
    }
  },
  "modelMappings": {
    "anthropicToOpenAI": {
      "claude-3-5-sonnet-20241022": "gpt-4o",
      "claude-3-5-haiku-20241022": "gpt-4o-mini",
      "claude-3-opus-20240229": "gpt-4-turbo",
      "claude-3-sonnet-20240229": "gpt-4",
      "claude-3-haiku-20240307": "gpt-3.5-turbo"
    },
    "openAIToAnthropic": {
      "gpt-4o": "claude-3-5-sonnet-20241022",
      "gpt-4o-mini": "claude-3-5-haiku-20241022",
      "gpt-4-turbo": "claude-3-opus-20240229",
      "gpt-4": "claude-3-sonnet-20240229",
      "gpt-3.5-turbo": "claude-3-haiku-20240307"
    },
    "openAICompatibles": {
      "gpt-4o": [
        "anthropic/claude-3.5-sonnet",
        "anthropic/claude-sonnet-4",
        "google/gemini-2.5-pro-preview"
      ],
      "gpt-4": [
        "anthropic/claude-3-sonnet-20240229",
        "deepseek/deepseek-chat-v3-0324"
      ],
      "gpt-3.5-turbo": [
        "claude-3-haiku-20240307",
        "kimi-k2-0711-preview"
      ]
    }
  },
  "routingConfig": {
    "defaultModel": "gemini-cli,gemini-2.5-pro",
    "backgroundModel": "gemini-cli,gemini-2.5-flash",
    "thinkModel": "gemini-cli,gemini-2.5-pro",
    "longContextModel": "gemini-cli,gemini-2.5-pro",
    "webSearchModel": "gemini-cli,gemini-2.5-flash"
  }
}