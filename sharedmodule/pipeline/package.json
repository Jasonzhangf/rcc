{
  "name": "rcc-pipeline",
  "version": "0.1.1",
  "description": "RCC Pipeline Module System - A comprehensive pipeline scheduling and load balancing system built on BaseModule",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "type": "commonjs",
  "scripts": {
    "build": "npm run clean && npm run build:simple",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:bundle": "rollup -c",
    "build:simple": "npx rollup -c rollup-simple.config.mjs",
    "clean": "rimraf dist",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "prebuild": "npm run build:simple",
    "prepublishOnly": "echo 'Skipping prepublish validation'",
    "release": "npm run test && npm run build:simple && npm publish",
    "release:dry": "npm run test && npm run build:simple && npm publish --dry-run"
  },
  "keywords": [
    "pipeline",
    "workflow",
    "scheduling",
    "load-balancing",
    "typescript",
    "rcc",
    "microservices",
    "task-orchestration",
    "qwen",
    "ai",
    "llm"
  ],
  "author": "RCC Development Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rcc/rcc-pipeline.git"
  },
  "homepage": "https://github.com/rcc/rcc-pipeline#readme",
  "bugs": {
    "url": "https://github.com/rcc/rcc-pipeline/issues"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "openai-compatible-providers-framework": "^0.0.7",
    "rcc-basemodule": "^0.1.3",
    "rcc-errorhandling": "^1.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^1.0.0",
    "@vitest/ui": "^1.0.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  }
}
