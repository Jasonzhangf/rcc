{
  "name": "rcc-server",
  "version": "0.1.8",
  "description": "RCC Server Module - Client input proxy response server with virtual model routing",
  "type": "module",
  "main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rcc/rcc-server.git"
  },
  "homepage": "https://github.com/rcc/rcc-server#readme",
  "bugs": {
    "url": "https://github.com/rcc/rcc-server/issues"
  },
  "keywords": [
    "rcc",
    "server",
    "proxy",
    "virtual-model",
    "routing",
    "typescript",
    "http-server",
    "modular-architecture"
  ],
  "author": "RCC Development Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "rcc-basemodule": "^0.1.8",
    "rcc-config-parser": "^0.1.0",
    "rcc-errorhandling": "^1.0.3",
    "rcc-pipeline": "^0.1.0",
    "rcc-underconstruction": "file:../underconstruction",
    "rcc-virtual-model-rules": "^1.0.5",
    "uuid": "^9.0.1"
  },
  "peerDependencies": {
    "rcc-basemodule": ">=0.1.0",
    "rcc-config-parser": ">=0.1.0",
    "rcc-errorhandling": ">=1.0.0",
    "rcc-pipeline": ">=0.1.0",
    "rcc-underconstruction": ">=0.1.0",
    "rcc-virtual-model-rules": ">=1.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@types/body-parser": "^1.19.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "rollup": "^4.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-typescript2": "^0.36.0",
    "ts-jest": "^29.1.1",
    "typedoc": "0.25.13",
    "typescript": "^5.9.2"
  },
  "scripts": {
    "build": "npm run clean && npm run build:types && npm run build:cjs && npm run build:esm",
    "build:types": "tsc --declaration --emitDeclarationOnly --outDir dist",
    "build:cjs": "rollup -c rollup.config.js",
    "build:esm": "rollup -c rollup.config.js",
    "clean": "rimraf dist",
    "dev": "tsc --watch",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write \"src/**/*.ts\" \"__test__/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"__test__/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "docs:generate": "typedoc src --out docs --theme default",
    "docs:serve": "http-server docs -p 8080 -o",
    "validate": "npm run typecheck && npm run lint && npm run test",
    "prepublishOnly": "npm run build",
    "version": "npm run build && git add -A dist",
    "postversion": "git push && git push --tags"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "@typescript-eslint/recommended"
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-explicit-any": "warn",
      "@typescript-eslint/explicit-function-return-type": "warn"
    },
    "env": {
      "node": true,
      "jest": true
    }
  },
  "prettier": {
    "semi": true,
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100,
    "tabWidth": 2,
    "useTabs": false
  }
}
